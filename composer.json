{
    "name": "thebachtiarz-company/laravel-auth-v2",
    "description": "An Auth Library for Laravel Project v2",
    "type": "library",
    "version": "1.0.5",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.14",
        "laravel/sanctum": "^3.2",
        "thebachtiarz-company/laravel-base-v1": "^1.0.28"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheBachtiarz\\Auth\\": "src/"
        },
        "files": [
            "helper/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheBachtiarz\\Auth\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "thebachtiarz-company",
            "email": "thebachtiarz.company@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "thebachtiarz",
        "auth"
    ],
    "homepage": "https://github.com/thebachtiarz-company/laravel-auth-v2",
    "support": {
        "issues": "https://github.com/thebachtiarz-company/laravel-auth-v2/issues",
        "source": "https://github.com/thebachtiarz-company/laravel-auth-v2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
